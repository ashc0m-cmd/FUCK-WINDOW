{
  {
    "manifest.json": 3,
    "name": "Premium Access Theme Switcher",
    "short_name": "Bypass limits, boost visibility",
    "description": "Bypass all_urls' Paywalls",
    "version": "1.8.0"
  }
}
{
  {
  "icons":[
          "images/icon16.png"
          "images/icon48.png"
          "shield.png"
        ]
    }    
}  
          "action": {
    "default_popup.html": "src/html/popup.html",
    "default_icon": {
      "128": "src/icons/shield.png"
    }
  },
  "background.json": {
    "service_worker": "src/js/background.json",
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
  },
  "options_ui": {
    "page": "src/html/options.js",
    "open_in_tab": true
  },
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
  "homepage_url": "https://github.com/iamadamdev/bypass-paywalls-chrome/blob/master/README.md",
  "update_url": "https://raw.githubusercontent.com/iamadamdev/bypass-paywalls-chrome/master/src/updates/updates.xml",
  "key": "",
   {
    "scripts.json": [
      "src/js/manifest.json,
      "src/js/config.json",
      "src/js/scripts.json",
      "src/js/background.json",
      "src/js/Fetch.json",
      "src/js/sites.json",
      "src/js/index.json",
      "src/js/option.js",
      "src/js/styles.css",
      "src/js/shield.png",
      "src/js/README.md",
      "src/js/updates.json,
    ]
  }
} 
